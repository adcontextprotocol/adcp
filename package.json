{
  "name": "adcontextprotocol",
  "version": "2.4.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "bash scripts/start-docusaurus.sh",
    "start:mintlify": "bash scripts/start-mintlify.sh",
    "build": "docusaurus build",
    "build:mintlify": "cd mintlify-docs && mintlify build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "deploy:mintlify": "mintlify deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "test:schemas": "node tests/schema-validation.test.js",
    "test:examples": "node tests/example-validation-simple.test.js",
    "generate-openapi": "node scripts/generate-openapi.js",
    "test": "npm run test:schemas && npm run test:examples && npm run typecheck",
    "precommit": "npm test",
    "prepare": "husky",
    "changeset": "changeset",
    "version": "changeset version && npm run update-schema-versions",
    "update-schema-versions": "node scripts/update-schema-versions.js",
    "release": "npm run version && npm test && npm run verify-version-sync && git add -A && git commit -m 'Version packages' && git push",
    "verify-version-sync": "node scripts/verify-version-sync.js"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.1",
    "@docusaurus/plugin-client-redirects": "^3.9.1",
    "@docusaurus/preset-classic": "^3.9.1",
    "@docusaurus/theme-mermaid": "^3.9.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "docusaurus-plugin-llms": "^0.2",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@docusaurus/module-type-aliases": "^3.9.1",
    "@docusaurus/tsconfig": "^3.9.1",
    "@docusaurus/types": "^3.9.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "glob": "^11.0.3",
    "husky": "^9.0.11",
    "mintlify": "^4.2.177",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "overrides": {
    "webpack-dev-server": "^5.2.2"
  }
}

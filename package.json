{
  "name": "adcontextprotocol",
  "version": "2.4.0",
  "private": true,
  "scripts": {
    "start": "tsx watch server/src/index.ts",
    "start:mcp": "MODE=mcp tsx watch server/src/index.ts",
    "start:mintlify": "bash scripts/start-mintlify.sh",
    "build": "tsc --project server/tsconfig.json",
    "build:mintlify": "cd mintlify-docs && mintlify build",
    "deploy:mintlify": "mintlify deploy",
    "typecheck": "tsc --project server/tsconfig.json --noEmit",
    "test:schemas": "node tests/schema-validation.test.js",
    "test:examples": "node tests/example-validation-simple.test.js",
    "test:registry": "vitest run server/tests",
    "generate-openapi": "node scripts/generate-openapi.js",
    "test": "npm run test:schemas && npm run test:examples && npm run typecheck",
    "precommit": "npm test",
    "prepare": "husky",
    "changeset": "changeset",
    "version": "changeset version && npm run update-schema-versions",
    "update-schema-versions": "node scripts/update-schema-versions.js",
    "release": "npm run version && npm test && npm run verify-version-sync && git add -A && git commit -m 'Version packages' && git push",
    "verify-version-sync": "node scripts/verify-version-sync.js"
  },
  "dependencies": {
    "@adcp/client": "^2.7.2",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/supertest": "^6.0.3",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "c8": "^10.1.3",
    "glob": "^11.0.3",
    "husky": "^9.0.11",
    "mintlify": "^4.2.177",
    "msw": "^2.11.6",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0",
    "typescript": "~5.6.2",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": ">=18.0"
  },
  "overrides": {
    "webpack-dev-server": "^5.2.2"
  }
}

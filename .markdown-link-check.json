{
  "ignorePatterns": [
    {
      "pattern": "^mailto:"
    },
    {
      "pattern": "^#"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^docs/",
      "replacement": "https://adcontextprotocol.org/docs/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://adcontextprotocol.org"],
      "headers": {
        "Accept": "text/html"
      }
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 3,
  "aliveStatusCodes": [200, 206]
}

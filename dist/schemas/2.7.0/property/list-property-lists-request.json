{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "/schemas/2.7.0/property/list-property-lists-request.json",
  "title": "List Property Lists Request",
  "description": "Request parameters for listing property lists",
  "type": "object",
  "properties": {
    "principal": {
      "type": "string",
      "description": "Filter to lists owned by this principal"
    },
    "name_contains": {
      "type": "string",
      "description": "Filter to lists whose name contains this string"
    },
    "max_results": {
      "type": "integer",
      "description": "Maximum lists to return",
      "minimum": 1,
      "default": 100
    },
    "cursor": {
      "type": "string",
      "description": "Pagination cursor"
    },
    "context": {
      "$ref": "/schemas/2.7.0/core/context.json"
    },
    "ext": {
      "$ref": "/schemas/2.7.0/core/ext.json"
    }
  },
  "additionalProperties": false
}
